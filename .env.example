# Trace Collection Service Configuration

# Jaeger Connection
JAEGER_QUERY_HOST=localhost
JAEGER_QUERY_PORT=16686
SERVICE_NAME=frontend-service

# Collection Settings
POLL_INTERVAL=3600              # Collection interval in seconds (1 hour)
PAST_INTERVAL=3600              # Time window to collect in seconds (1 hour)
LATENCY_PERCENTILE=99           # Anomaly threshold percentile
MAX_TRACES=10000                # Maximum traces per collection
